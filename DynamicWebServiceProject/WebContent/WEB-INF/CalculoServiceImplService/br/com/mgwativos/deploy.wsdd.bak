<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculoServiceImplService WSDL service -->

  <service name="CalculoServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://mgwativos.com.br"/>
      <parameter name="wsdlServiceElement" value="CalculoServiceImplService"/>
      <parameter name="schemaQualified" value="http://mgwativos.com.br"/>
      <parameter name="wsdlServicePort" value="CalculoServiceImpl"/>
      <parameter name="className" value="br.com.mgwativos.CalculoServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculoServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="somar" qname="operNS:somar" xmlns:operNS="http://mgwativos.com.br" returnQName="retNS:somarReturn" xmlns:retNS="http://mgwativos.com.br" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://mgwativos.com.br" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://mgwativos.com.br" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="somar"/>

  </service>
</deployment>
